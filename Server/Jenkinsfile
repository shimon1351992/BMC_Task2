pipeline {
    agent {
       label 'slave1'   
    }

    stages {
        stage('directory') {
            steps {
                echo 'Building..'
                sh """ls"""
            }
        }
        
        stage('build') {
            steps {
               
                sh '''#!/usr/bin/env bash
                Server/Server.java'''
            }
        }
        stage('Run') {
            steps {
              sh """
              cd Server
              java Server
              """
            }
        }
    
  
  } 
}
