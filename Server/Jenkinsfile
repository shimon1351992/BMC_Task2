pipeline {
    agent {
       label 'slave1'   
    }

    stages {
        stage('directory') {
            steps {
                echo 'Building..'
                sh """ls"""
            }
        }
          stages {
        stage('Build') {
            steps {
                sh 'Server/Server.java'
            }
        }
        stage('Run') {
            steps {
              sh """
              cd Server
              java Server ${HOSTNAME} ${PATH1}
              """
            }
        }
    }
  
  } 
}
