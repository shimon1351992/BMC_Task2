pipeline {
    agent {
       label 'ssh-agent'   
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                bat 'javac Server/Server.java'
            }
        }
        stage('Run') {
            steps {
               bat """
               cd Server
               java Server
               """
        }
    }
  } 
}
