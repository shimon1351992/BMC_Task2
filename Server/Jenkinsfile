pipeline {
    agent {
       label 'slave1'   
    }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'javac /home/jenkins/workspace/client/Client/Server/Server.java'
            }
        }
        stage('Run') {
            steps {
               sh """
               cd Server
               java Server
               """
        }
    }
  } 
}
